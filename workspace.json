{
  "version": 2,
  "projects": {
    "testing": {
      "root": "apps/tester",
      "sourceRoot": "apps/tester",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "does-not-report": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "jest.config.js",
            "passWithNoTests": false,
            "silent": false,
            "color": false,
            "detectOpenHandles": true,
            "testTimeout": 3600000
          }
        },
        "reports": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "jest --detectOpenHandles --config jest.config.js --testTimeout 3600000"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
